A,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,GAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,4BAAA,EAAA,KAAA,CAAA,QAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,KAAA,CAAA,QAAA,CAAA;AACA,cAAA,IAAA,EAAA,SADA;AAEA,cAAA,OAAA,EAAA;AAFA,aAAA;;AAIA,YAAA,KAAA,CAAA,SAAA,CAAA,IAAA,GAAA,KAAA;;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,QAAA;AAEA,WATA;;AAUA,UAAA,KAAA,CAAA,QAAA,GAAA,EAAA;AACA,SAdA,MAcA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,iBAAA,KAAA;AACA;AACA,OAnBA;AAoBA;AAtBA;AAvBA,CAAA","sourcesContent":["<template>\n  <div class=\"hello\">\n    <el-dialog title=\"添加用户信息\" :visible.sync=\"dialogAdd.show\">\n      <el-form :model=\"formDate\" ref=\"formdong\" label-width=\"100px\" :rules=\"formrules\">\n        <el-form-item label=\"日期\" prop=\"date\">\n          <el-date-picker\n              v-model=\"formDate.date\"\n              type=\"date\"\n              placeholder=\"选择日期\">\n          </el-date-picker>\n        </el-form-item>\n        <el-form-item label=\"姓名\" prop=\"name\">\n          <el-input v-model=\"formDate.name\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"邮箱\" prop=\"email\">\n          <el-input v-model=\"formDate.email\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"手机号\" prop=\"title\">\n          <el-input v-model=\"formDate.title\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"评价\" prop=\"evaluate\">\n          <el-input v-model=\"formDate.evaluate\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"\" prop=\"state\">\n          <el-input v-model=\"formDate.state\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogAdd.show = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"dialogFormAdd('formdong')\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AddUser',\n  props:{\n    dialogAdd:Object\n  },\n  data () {\n    return {\n      formDate:{\n        id:'',\n        date:'',\n        name:'',\n        email:'',\n        title:'',\n        evaluate:'',\n        state:''\n      },\n      formrules:{\n        date:[{required:true,message:\"日期不能为空\",trigger:\"blur\"}],\n        name:[{required:true,message:\"用户名不能为空\",trigger:\"blur\"}],\n        email:[{required:true,message:\"邮箱不能为空\",trigger:\"blur\"}],\n      }\n    }\n  },\n  methods:{\n    dialogFormAdd(formdong) {\n      this.$refs[formdong].validate((valid) => {\n        if (valid) {\n          this.formDate.id=this.$store.state.sum+1;\n          this.$store.state.sum++\n          this.$axios.post('http://localhost:3000/data',this.formDate).then(res => {\n            console.log(res)\n            this.$message({\n              type:\"success\",\n              message:\"添加信息成功\"\n            })\n            this.dialogAdd.show = false;\n            this.$emit('update');\n\n          })\n          this.formDate  = \"\"\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      })\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/components"}]}